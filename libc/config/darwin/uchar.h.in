%%begin()

#if defined(__has_include) && __has_include(<stddef.h>)
#include <stddef.h> /* Included for size_t */
#endif /* has_include */

#if defined(__has_include) && !__has_include(<uchar.h>)
#include <sys/_types/_mbstate_t.h>
#if defined(__CHAR16_TYPE__)
typedef __CHAR16_TYPE__ char16_t;
#else
typedef uint_least16_t  char16_t;
#endif // !defined(_CHAR16_T)

#if defined(__CHAR32_TYPE__)
typedef __CHAR32_TYPE__ char32_t;
#else
typedef uint_least32_t  char32_t;
#endif // !defined(__CHAR32_TYPE__)
#endif /* has_include */
